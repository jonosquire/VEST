  FontSize->13,
  FontWeight->"Bold"],
 StyleBox["and",
  FontFamily->"Helvetica"],
 StyleBox[" gradv ",
  FontFamily->"Courier",
  FontSize->13,
  FontWeight->"Bold"],
 StyleBox["functions can be input as ",
  FontFamily->"Helvetica"],
 "\[EscapeKey]",
 StyleBox["v.",
  FontFamily->"Courier",
  FontSize->13,
  FontWeight->"Bold"],
 "\[EscapeKey], \[EscapeKey]",
 StyleBox["v*",
  FontFamily->"Courier",
  FontSize->13,
  FontWeight->"Bold"],
